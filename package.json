{
  "name": "@nuxthealth/node-dicom",
  "version": "0.1.0",
  "type": "commonjs",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "binaryName": "node-dicom",
    "targets": [
      "aarch64-apple-darwin",
      "aarch64-unknown-linux-gnu",
      "x86_64-apple-darwin",
      "x86_64-pc-windows-msvc",
      "x86_64-unknown-linux-gnu"
    ]
  },
  "license": "MIT",
  "files": [
    "index.d.ts",
    "index.js"
  ],
  "devDependencies": {
    "@emnapi/core": "^1.7.1",
    "@emnapi/runtime": "^1.7.1",
    "@napi-rs/cli": "^3.5.0",
    "@oxc-node/core": "^0.0.35",
    "@taplo/cli": "^0.7.0",
    "@tybys/wasm-util": "^0.10.1",
    "ava": "^6.4.1",
    "chalk": "^5.6.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "npm-run-all2": "^8.0.4",
    "oxlint": "^1.32.0",
    "prettier": "^3.7.4",
    "tinybench": "^6.0.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">= 10"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --release --platform --features transcode",
    "build:debug": "napi build --platform --features transcode",
    "format": "run-p format:prettier format:rs format:toml",
    "format:prettier": "prettier . -w",
    "format:toml": "taplo format",
    "format:rs": "cargo fmt",
    "lint": "oxlint .",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "universal": "napi universal",
    "version": "napi version",
    "bumpp": "bumpp"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "timeout": "2m",
    "workerThreads": false,
    "environmentVariables": {
      "OXC_TSCONFIG_PATH": "./__test__/tsconfig.json"
    },
    "nodeArguments": [
      "--import",
      "@oxc-node/core/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:DevJoghurt/node-dicom-rs.git"
  },
  "packageManager": "yarn@4.12.0"
}
